{
    "name": "lpy",
    "displayName": "lpy",
    "description": "Syntax highlighting for l-py python language",
    "version": "0.0.2",
    "author": {
        "email": "j-magnusson@t-online.de",
        "name": "Jannes Magnusson"
    },
    "publisher": "jannessm",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/jannessm/lpy-highlighting"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lpy",
            "aliases": ["lpy", "l-py"],
            "extensions": [".lpy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lpy",
            "scopeName": "source.lpy",
            "path": "./syntaxes/lpy.tmLanguage.json",
            "embeddedLanguages": {
                "source.python": "python"
            }
        }]
    }
}